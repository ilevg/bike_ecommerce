{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/products",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/customers",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/orders",
      "dest": "/api/server.js"
    },
    {
      "src": "/api/register",
      "dest": "/api/server.js"
    }
  ]
}
